<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>

<VBox spacing="10" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.bsu.cs222.GUIView"
      prefWidth="500" prefHeight="400"
      alignment="CENTER">

    <TextField fx:id="inputField" promptText="Enter article title"/>
    <Button fx:id="fetchButton" text="Fetch Revisions"/>
    <TextArea fx:id="outputArea" prefHeight="300"/>
    <Label fx:id="label"/>
</VBox>
